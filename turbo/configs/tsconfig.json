{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["ES2017"],
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "outDir": "../../resources/[turbo]/",
    "removeComments": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6",
    "types": ["@citizenfx/client", "@citizenfx/server", "@types/node"]
  },
  "exclude": ["node_modules", "**/*.d.ts"]
}
